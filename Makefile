CFLAGS := -g -Og -fno-omit-frame-pointer
CXXFLAGS := $(CFLAGS)
LLDB := lldb

.PHONY: all
all: NCOM POLY GZIP BC PEG YAML1 YAML2 TIFF

.PHONY: NCOM, RUN-NCOM, RUN-LLDB-NCOM
NCOM:
	git clean -xfd ncompress-4.2.4; git restore ncompress-4.2.4
	cd ncompress-4.2.4; cp Makefile.def Makefile; bear -- make -j options="$(CFLAGS) -std=gnu90 -DDIRENT=1 -DUSERMEM=800000 -DREGISTERS=3 -DNOFUNCDEF=1"
RUN-NCOM:
	rm -f ../_.Z
	cd ncompress-4.2.4; ./compress ../_
RUN-LLDB-NCOM:
	rm -f ../_.Z
	cd ncompress-4.2.4; $(LLDB) -o run -- ./compress ../_

.PHONY: POLY, RUN-POLY, RUN-LLDB-POLY
POLY:
	git clean -xfd polymorph-0.4.0; git restore polymorph-0.4.0
	cd polymorph-0.4.0; autoupdate; aclocal; autoconf; automake --add-missing; CFLAGS="$(CFLAGS)" bash configure; bear -- make -j
RUN-POLY:
	cd polymorph-0.4.0; ./polymorph -f 
RUN-LLDB-POLY:
	cd polymorph-0.4.0; $(LLDB) -o run -- ./polymorph -f 

.PHONY: GZIP, RUN-GZIP, RUN-LLDB-GZIP
GZIP:
	git clean -xfd gzip-1.2.4; git restore gzip-1.2.4
	cd gzip-1.2.4; CFLAGS="$(CFLAGS) -std=gnu90" bash configure; bear -- make -j
RUN-GZIP:
	cd gzip-1.2.4; ./gzip ../_
RUN-LLDB-GZIP:
	cd gzip-1.2.4; $(LLDB) -o run -- ./gzip ../_

.PHONY: BC, RUN-BC, RUN-LLDB-BC
BC:
	git clean -xfd bc-1.06; git restore bc-1.06
	cd bc-1.06; aclocal; automake --add-missing; CFLAGS="$(CFLAGS) -std=gnu90" bash configure; bear -- make -j CFLAGS="$(CFLAGS) -std=gnu90"
RUN-BC:
	cd bc-1.06; ./bc/bc ../_input/BC/bad.b
RUN-LLDB-BC:
	cd bc-1.06; $(LLDB) -o run -- ./bc/bc ../_input/BC/bad.b

.PHONY: SQUD
SQUD:
	git clean -xfd squid-2.3; git restore squid-2.3
	cd squid-2.3; CFLAGS="$(CFLAGS)" bash configure; bear -- make -j

.PHONY: CVS
CVS:
	git clean -xfd cvs-1.11.4; git restore cvs-1.11.4
	cd cvs-1.11.4; bash noautomake.sh --noautoconf; CFLAGS="$(CFLAGS) -m32" bash configure; bear -- make -j

.PHONY: YPSV
YPSV:
	git clean -xfd ypserv-2.2; git restore ypserv-2.2
	cd ypserv-2.2; autoreconf -i; CFLAGS="-I/usr/include/tirpc $(CFLAGS)" LIBS="-ltirpc" bash configure; bear -- make -j

.PHONY: PEG, RUN-PEG, RUN-LLDB-PEG
PEG:
	git clean -xfd cpp-peglib-0.1.12; git restore cpp-peglib-0.1.12
	cmake cpp-peglib-0.1.12 -B cpp-peglib-0.1.12/build -DCMAKE_CXX_FLAGS="$(CXXFLAGS)" -DCMAKE_EXPORT_COMPILE_COMMANDS=ON; cmake --build cpp-peglib-0.1.12/build --parallel; cp cpp-peglib-0.1.12/build/compile_commands.json cpp-peglib-0.1.12
RUN-PEG:
	cd ./cpp-peglib-0.1.12; ./build/lint/peglint --ast --opt ../_input/PEG/SEGV-optimize-peglib-3650 ./pl0/samples/fib.pas
RUN-LLDB-PEG:
	cd ./cpp-peglib-0.1.12; $(LLDB) -o run -- ./build/lint/peglint --ast --opt ../_input/PEG/SEGV-optimize-peglib-3650 ./pl0/samples/fib.pas

.PHONY: YAML1, RUN-YAML1, RUN-LLDB-YAML1
YAML1:
	git clean -xfd yaml-cpp-0.6.3; git restore yaml-cpp-0.6.3
	cmake yaml-cpp-0.6.3 -B yaml-cpp-0.6.3/build -DCMAKE_CXX_FLAGS="$(CXXFLAGS)" -DCMAKE_EXPORT_COMPILE_COMMANDS=ON; cmake --build yaml-cpp-0.6.3/build --parallel; cp yaml-cpp-0.6.3/build/compile_commands.json yaml-cpp-0.6.3
RUN-YAML1:
	cd yaml-cpp-0.6.3; ./build/util/parse < ../_input/YAML1/crash2.txt
RUN-LLDB-YAML1:
	cd yaml-cpp-0.6.3; $(LLDB) -o "settings set target.input-path ../_input/YAML1/crash2.txt" -o run -- ./build/util/parse

.PHONY: YAML2, RUN-YAML2, RUN-LLDB-YAML2
YAML2:
	git clean -xfd yaml-cpp-0.6.2; git restore yaml-cpp-0.6.2
	cmake yaml-cpp-0.6.2 -B yaml-cpp-0.6.2/build -DCMAKE_CXX_FLAGS="$(CXXFLAGS)" -DCMAKE_EXPORT_COMPILE_COMMANDS=ON; cmake --build yaml-cpp-0.6.2/build --parallel; cp yaml-cpp-0.6.2/build/compile_commands.json yaml-cpp-0.6.2
RUN-YAML2:
	cd yaml-cpp-0.6.2; ./build/util/parse < ../_input/YAML2/file_poc
RUN-LLDB-YAML2:
	cd yaml-cpp-0.6.2; $(LLDB) -o "settings set target.input-path ../_input/YAML2/file_poc" -o run -- ./build/util/parse

.PHONY: TIFF, RUN-TIFF, RUN-LLDB-TIFF
TIFF:
	git clean -xfd libtiff-4.0.6; git restore libtiff-4.0.6
ifeq ($(shell uname -p), aarch64)
	cd libtiff-4.0.6; CFLAGS="$(CFLAGS)" CXXFLAGS="$(CXXFLAGS)" ./configure --build=aarch64-unknown-linux-gnu; bear -- make -j
else
	cd libtiff-4.0.6; CFLAGS="$(CFLAGS)" CXXFLAGS="$(CXXFLAGS)" ./configure; bear -- make -j
endif
RUN-TIFF:
	cd libtiff-4.0.6; ./tools/.libs/rgb2ycbcr -c zip -r 0 -h 2 -v 0 ../_input/TIFF/plasma-globe.tiff $(shell mktemp)
RUN-LLDB-TIFF:
	cd libtiff-4.0.6; $(LLDB) -o run -- ./tools/.libs/rgb2ycbcr -c zip -r 0 -h 2 -v 0 ../_input/TIFF/plasma-globe.tiff $(shell mktemp)

.PHONY: clean
clean:
	git clean -xfd .; git restore .
